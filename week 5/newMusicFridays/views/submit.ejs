<%- include ('./partials/header.ejs', {pageName: "Submit a Song" }) %>

<div id="card">
    <h1> Submit your favorite song!</h1>
    <hr>
        <form method="POST" action="./songSubmissions">
            <label for="songName"> Song Name </label>
            <input type="text" name="songName" id="songName" />
            <label for="artistName"> Artist Name </label>
            <input type="text" name="artistName" id="artistName" />
            <label for="songCategory"> Artist Name </label>
            <input type="text" name="songCategory" id="songCategory" />
            <label for="dateAdded"> Release Day </label>
            <input type="text" name="dateAdded" id="dateAdded" />
            <label for="songRating"> Rating </label>
            <input type="text" name="songRating" id="songRating" />

            <input type="submit" value="Submit New Music!">
            <hr>
          </form>
</div> 



